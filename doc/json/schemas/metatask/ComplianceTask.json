{
  "title": "mc_tasks::ComplianceTask",
  "type": "object",
  "properties":
  {
    "type": { "enum": ["compliance"] },
    "name": { "type": "string" },
    "robotIndex": { "type": "number", "minimum": 0 },
    "body": { "type": "string" },

    "wrench": { "$ref": "/../../sva/ForceVecd.json" },

    "dof": { "$ref": "/../../eigen/Matrix6d.json" },
    "forceThresh": { "type": "number" },
    "torqueThresh": { "type": "number" },
    "forceGain": { "type": "array", "members": { "type": "number" }, "minItems": 2, "maxItems": 2 },
    "torqueGain": { "type": "array", "members": { "type": "number" }, "minItems": 2, "maxItems": 2 },


    "stiffness": { "type": "number", "minimum": 0},
    "damping": { "type": "number", "minimum": 0},
    "weight": { "type": "number", "minimum": 0},
    "dimWeight": { "$ref": "/../../eigen/Vector3d.json" },
    "activeJoints": { "type": "array", "items": { "type": "string" } },
    "unactiveJoints": { "type": "array", "items": { "type": "string" } }
  },
  "required": ["type", "robotIndex", "body"]
}
